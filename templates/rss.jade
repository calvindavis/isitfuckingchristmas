doctype xml
rss(version="2.0")
	channel
		title Is it fucking Christmas?
		link http://isitfuckingchristmas.co.uk/
		description Is it fucking Christmas?
		language en-gb		
		- var first = items[0]
		pubDate #{first.pubDate}
		lastBuildDate #{first.pubDate}		
		- for (var i in items)
			- var item = items[i];
			item
				title #{item.description}
				description #{item.description}
				pubDate #{item.pubDate}
				link #{item.link}
				guid #{item.link}